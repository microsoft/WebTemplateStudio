{
  "name": "extension",
  "displayName": "Web Template Studio",
  "description": "An extension that helps developers get started quickly when building web apps by generating web templates which encourage best code practices.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:webTemplateStudioExtension.wizardLaunch"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "webTemplateStudioExtension.wizardLaunch",
        "title": "%webTemplateStudioExtension.commands.wizardLaunch%",
        "category": "%webTemplateStudioExtension.commands.wts%"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/node": "^8.10.25",
    "tslint": "^5.8.0",
    "typescript": "^3.1.4",
    "vscode": "^1.1.25"
  },
  "extensionDependencies": [
    "ms-azuretools.vscode-cosmosdb"
  ],
  "dependencies": {
    "@types/jest": "^24.0.5",
    "@types/mocha": "^5.2.6",
    "azure-arm-resource": "^7.3.0"
  }
}
