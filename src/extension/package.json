{
  "name": "__EXTENSIONNAME__",
  "displayName": "__DISPLAYNAME__",
  "description": "An extension that helps developers get started quickly when building web apps by generating web templates which encourage best code practices.",
  "version": "__VERSIONNUMBER__",
  "publisher": "__PUBLISHER__",
  "aiKey": "__AIKEY__",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "galleryBanner": {
    "color": "#3c3c3c",
    "theme": "dark"
  },
  "icon": "",
  "keywords": [
    "Garage",
    "Web Application"
  ],
  "preview": true,
  "homepage": "https://github.com/Microsoft/WebTemplateStudio/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/Microsoft/WebTemplateStudio/issues"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/WebTemplateStudio"
  },
  "activationEvents": [
    "onCommand:webTemplateStudioExtension.wizardLaunch"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "webTemplateStudioExtension.wizardLaunch",
        "title": "%webTemplateStudioExtension.commands.wizardLaunch%",
        "category": "%webTemplateStudioExtension.commands.wts%"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "createVsixPackage": "rm -rf ../../dist/wts.vsix && vsce package --out ../../dist/wts.__VERSIONNUMBER__.vsix"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
